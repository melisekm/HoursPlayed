<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="401.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="startBtn" layoutX="112.0" layoutY="262.0" mnemonicParsing="false" onAction="#startBtnAction" prefHeight="45.0" prefWidth="86.0" text="START">
               <font>
                  <Font name="Lucida Console" size="18.0" />
               </font>
            </Button>
            <Button fx:id="stopBtn" layoutX="428.0" layoutY="262.0" mnemonicParsing="false" onAction="#stopBtnAction" prefHeight="45.0" prefWidth="86.0" text="STOP">
               <font>
                  <Font name="Lucida Console" size="18.0" />
               </font>
            </Button>
            <Label fx:id="playingLabel" layoutX="266.0" layoutY="205.0" prefHeight="35.0" prefWidth="94.0" text="PLAYING" textFill="#0067ff">
               <font>
                  <Font name="Lucida Console" size="20.0" />
               </font>
            </Label>
            <Label fx:id="notPlayingLabel" layoutX="243.0" layoutY="210.0" prefHeight="25.0" prefWidth="139.0" text="NOT PLAYING" textFill="RED">
               <font>
                  <Font name="Lucida Console" size="20.0" />
               </font>
            </Label>
            <Label fx:id="dateTwoWeeks" layoutX="178.0" layoutY="170.0" prefHeight="35.0" prefWidth="284.0" text="XX.XX.XXXX - XX.XX.XXXX">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label layoutX="164.0" layoutY="99.0" prefHeight="71.0" prefWidth="312.0" text="HOURS PAST 2 WEEEKS">
               <font>
                  <Font size="29.0" />
               </font>
            </Label>
            <Label fx:id="total" layoutX="14.0" layoutY="16.0">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="dateOrig" layoutX="14.0" layoutY="46.0">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Label fx:id="hrsCurr" layoutX="150.0" layoutY="43.0">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
            <Label fx:id="sessionLabel" layoutX="166.0" layoutY="344.0">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button fx:id="refreshBtn" layoutX="292.0" layoutY="319.0" mnemonicParsing="false" onAction="#refreshAction" text="Refresh" />
            <Button fx:id="sessionListBtn" layoutX="511.0" layoutY="198.0" mnemonicParsing="false" onAction="#sessionListBtnAction" />
      </children>
    </AnchorPane>
  </children>
</VBox>
